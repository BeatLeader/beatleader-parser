<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net48</TargetFrameworks>
    <RootNamespace>beatleader_parser</RootNamespace>
	<LangVersion>10.0</LangVersion>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<Title>Beat Saber song parser library</Title>
	<Authors>Loloppe, Light Ai</Authors>
	<EnableNETAnalyzers>True</EnableNETAnalyzers>
	<AnalysisLevel>latest-all</AnalysisLevel>
	<EnforceCodeStyleInBuild>True</EnforceCodeStyleInBuild>
	<AssemblyVersion>0.1.0</AssemblyVersion>
	<PackageProjectUrl>https://github.com/BeatLeader/beatleader-parser</PackageProjectUrl>
	<RepositoryUrl>https://github.com/BeatLeader/beatleader-parser.git</RepositoryUrl>
	<PackageId>$(AssemblyName)</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="NVorbis" Version="0.10.5" />
    <PackageReference Include="System.IO.Compression" Version="4.3.0" />
  </ItemGroup>

</Project>
